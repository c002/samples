       s.append(this.getBillLname()); s.append(sep); 
       s.append(this.getBillAddress1()); s.append(sep); 
       s.append(this.getBillAddress2()); s.append(sep); 
       s.append(this.getBillAddress3()); s.append(sep); 
       s.append(this.getBillSuburb()); s.append(sep); 
       s.append(this.getBillState()); s.append(sep); 
       s.append(this.getBillPostCode()); s.append(sep); 
       s.append(this.getCustAddress1()); s.append(sep); 
       s.append(this.getCustAddress2()); s.append(sep); 
       s.append(this.getCustAddress3()); s.append(sep); 
       s.append(this.getCustSuburb()); s.append(sep); 
       s.append(this.getCustState()); s.append(sep); 
       s.append(this.getCustPostCode()); s.append(sep); 
       s.append("\n");
             
       return(s.toString());
   }
   
